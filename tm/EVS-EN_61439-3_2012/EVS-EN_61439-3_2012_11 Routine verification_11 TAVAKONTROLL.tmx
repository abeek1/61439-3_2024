    <tu>
      <tuv xml:lang="en">
        <seg>Routine verification</seg>
      </tuv>
      <tuv xml:lang="et" creationid="OmegaT Aligner" creationdate="20240405T133404Z">
        <seg>TAVAKONTROLL</seg>
      </tuv>
    </tu>
    <tu>
